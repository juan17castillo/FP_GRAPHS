<?xml version="1.0" encoding="UTF-8"?>

<?import com.lynden.gmapsfx.GoogleMapView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainViewController">
   <children>
      <AnchorPane fx:id="anchorInit" prefHeight="600.0" prefWidth="1200.0" visible="false" />
      <AnchorPane fx:id="anchorApp" prefHeight="200.0" prefWidth="200.0">
         <children>
            <BorderPane prefHeight="600.0" prefWidth="1200.0">
               <center>
                  <GoogleMapView fx:id="mapView" prefHeight="252.0" prefWidth="259.0" BorderPane.alignment="CENTER" />
               </center>
               <left>
                  <AnchorPane prefHeight="600.0" prefWidth="238.0" stylesheets="@application.css" BorderPane.alignment="CENTER">
                     <children>
                        <AnchorPane fx:id="anchorButtons" layoutX="14.0" layoutY="200.0" prefHeight="388.0" prefWidth="206.0">
                           <children>
                              <Button fx:id="btnCreateTour" layoutX="14.0" layoutY="30.0" mnemonicParsing="false" onAction="#createTour" prefHeight="60.0" prefWidth="184.0" text="Crear Tour" />
                              <Button fx:id="btnRamdonTour" layoutX="11.0" layoutY="145.0" mnemonicParsing="false" onAction="#generateRamdonToyr" prefHeight="60.0" prefWidth="185.0" text="Generar Tour" />
                              <Button fx:id="btnBack" layoutX="6.0" layoutY="253.0" mnemonicParsing="false" onAction="#goBack" prefHeight="64.0" prefWidth="195.0" text="Salir" />
                           </children>
                        </AnchorPane>
                        <AnchorPane fx:id="anchorOptions" layoutX="5.0" layoutY="183.0" prefHeight="412.0" prefWidth="214.0" visible="false">
                           <children>
                              <Text fill="#a8a8a8" layoutX="23.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Seleccione una ciudad de origen:" wrappingWidth="183.13671875" />
                              <ComboBox fx:id="comboFrom" layoutX="36.0" layoutY="34.0" prefWidth="150.0" promptText="Seleccione" />
                              <Text fill="#a8a8a8" layoutX="32.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ciudades que quiere visitar" wrappingWidth="158.13671875" />
                              <TableView fx:id="tableCities" layoutX="11.0" layoutY="106.0" prefHeight="200.0" prefWidth="200.0">
                                <columns>
                                  <TableColumn fx:id="columnCities" prefWidth="198.0" text="Ciudad" />
                                </columns>
                              </TableView>
                              <Button fx:id="btnAddCityTable" layoutY="312.0" mnemonicParsing="false" onAction="#addCityOnTable" prefHeight="30.0" prefWidth="112.0" text="Agregar ciudad">
                                 <font>
                                    <Font size="1.0" />
                                 </font>
                              </Button>
                              <Button fx:id="btnRemoveCityOnTable" layoutX="115.0" layoutY="312.0" mnemonicParsing="false" onAction="#removeCityOnTable" prefHeight="32.0" prefWidth="120.0" text="Eliminar Ciudad">
                                 <font>
                                    <Font size="1.0" />
                                 </font>
                              </Button>
                              <Button fx:id="btnGenerateTour" layoutX="24.0" layoutY="353.0" mnemonicParsing="false" onAction="#generateTour" prefHeight="50.0" prefWidth="179.0" text="Generar tour" />
                           </children>
                        </AnchorPane>
                        <ImageView fitHeight="150.0" fitWidth="199.0" layoutX="17.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../image/1.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </left>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
